{
  "dashboard": {
    "title": "Campus Pulse - Real-Time Monitoring",
    "tags": ["campus-pulse", "monitoring", "uf"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "campus_pulse_active_users",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Total Events",
        "type": "stat",
        "targets": [
          {
            "expr": "campus_pulse_total_events",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Average Crowd Level",
        "type": "gauge",
        "targets": [
          {
            "expr": "campus_pulse_avg_crowd_level_percent",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 0
        },
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "orange", "value": 80},
                {"color": "red", "value": 90}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 4,
        "title": "Locations Monitored",
        "type": "stat",
        "targets": [
          {
            "expr": "campus_pulse_locations_monitored",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Page Views (Last 5 minutes)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(campus_pulse_page_views_total[5m])",
            "legendFormat": "{{page_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 4
        },
        "options": {
          "tooltip": {
            "mode": "multi"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom"
          }
        }
      },
      {
        "id": 6,
        "title": "Model Predictions Rate",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(campus_pulse_model_predictions_total[5m])",
            "legendFormat": "{{model_type}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 4
        }
      },
      {
        "id": 7,
        "title": "Top 10 Busiest Locations",
        "type": "bargauge",
        "targets": [
          {
            "expr": "topk(10, campus_pulse_location_crowd_level)",
            "legendFormat": "{{location_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 12
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "fieldConfig": {
          "defaults": {
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 60},
                {"color": "red", "value": 85}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 8,
        "title": "Event Classifications by Category",
        "type": "piechart",
        "targets": [
          {
            "expr": "campus_pulse_event_classifications_total",
            "legendFormat": "{{category}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 12
        },
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          }
        }
      },
      {
        "id": 9,
        "title": "Model Prediction Latency (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(campus_pulse_model_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "{{model_type}} p95",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 20
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Latency"
          }
        ]
      },
      {
        "id": 10,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(campus_pulse_errors_total[5m])",
            "legendFormat": "{{error_type}} - {{component}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 20
        },
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [0.1],
                "type": "gt"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 11,
        "title": "User Activity (Signups & Logins)",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(campus_pulse_user_signups_total[5m])",
            "legendFormat": "Signups",
            "refId": "A"
          },
          {
            "expr": "rate(campus_pulse_user_logins_total[5m])",
            "legendFormat": "Logins",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 12,
        "title": "Database Query Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(campus_pulse_db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "{{query_type}} p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(campus_pulse_db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "{{query_type}} p50",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 26
        },
        "yaxes": [
          {
            "format": "s",
            "label": "Query Duration"
          }
        ]
      },
      {
        "id": 13,
        "title": "Anomalies Detected",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(campus_pulse_anomalies_detected_total[1h]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 32
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "title": "Cache Hit Rate",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(campus_pulse_cache_hits_total[5m]) / (rate(campus_pulse_cache_hits_total[5m]) + rate(campus_pulse_cache_misses_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 6,
          "y": 32
        },
        "fieldConfig": {
          "defaults": {
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "green", "value": 80}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 15,
        "title": "Crowd Level Heatmap by Location Type",
        "type": "heatmap",
        "targets": [
          {
            "expr": "avg by (location_type) (campus_pulse_location_crowd_level)",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "location_type",
          "type": "query",
          "query": "label_values(campus_pulse_location_crowd_level, location_type)",
          "label": "Location Type"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "datasource": "Prometheus",
          "enable": true,
          "expr": "ALERTS{alertstate=\"firing\"}",
          "iconColor": "red",
          "name": "Alerts",
          "titleFormat": "Alert"
        }
      ]
    }
  }
}
